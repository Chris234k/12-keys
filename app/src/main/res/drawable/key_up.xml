<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/dark_2"/>
            <stroke
                android:width="@dimen/key_stroke_width"
                android:color="@color/key_stroke_color"/>
            <corners
                android:radius="@dimen/key_radius"/>
        </shape>
    </item>
    <item
        android:bottom="25dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/light_0"/>
        </shape>
    </item>
</layer-list>