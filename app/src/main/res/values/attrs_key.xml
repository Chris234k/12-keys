<resources>
    <declare-styleable name="Key">
        <attr name="special" format="boolean"/>
        <attr name="tap" format="string" />
        <attr name="up" format="string" />
        <attr name="down" format="string" />
        <attr name="left" format="string" />
        <attr name="right" format="string" />

        <attr name="key_state_tap" format="boolean"/>
        <attr name="key_state_left" format="boolean"/>
        <attr name="key_state_up" format="boolean"/>
        <attr name="key_state_right" format="boolean"/>
        <attr name="key_state_down" format="boolean"/>
    </declare-styleable>

    <dimen name="key_radius">0dp</dimen>
    <dimen name="key_stroke_width">1dp</dimen>
    <color name="key_stroke_color">@color/dark_1</color>

    <dimen name="key_height">60dp</dimen>
</resources>